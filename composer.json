{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "anik/form-request": "^5.0",
        "darkaonline/swagger-lume": "^10.1",
        "flipbox/lumen-generator": "^9.2",
        "illuminate/mail": "^10.38",
        "illuminate/notifications": "^10.37",
        "illuminate/support": "^10.37",
        "laravel/lumen-framework": "^10.0",
        "maatwebsite/excel": "^3.1",
        "spatie/laravel-medialibrary": "^10.15",
        "spatie/laravel-permission": "^6.2",
        "swiftmailer/swiftmailer": "^5.4",
        "tymon/jwt-auth": "^2.0",
        "zircote/swagger-php": "3.*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
